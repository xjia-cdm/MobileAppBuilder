#!/bin/sh

HOME=`dirname $0`/..

LIBPATH=$HOME/lib/ext/groovy-all-2.4.0.jar:$HOME/lib/ext/ant-1.8.2.jar:$HOME/lib/ext/ant-launcher-1.8.2.jar:$HOME/lib/ext/commons-lang3-3.1.jar:$HOME/lib/ext/jyaml-1.3.jar

CLASSPATH==$HOME/work:$HOME/lib/appbuilder.jar:$HOME/lib/translator.jar:$LIBPATH 

java -Xms512m -Xmx4096m -classpath $CLASSPATH xj.mobile.Main $* 

exit 0
